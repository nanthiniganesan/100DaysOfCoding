# Read a string:
# s = input()
# Print a value:
# print(s)
n = int(input())
Location ={}
for i in range (n):
    a = list(input().split())
    Location[a[0]] = a[1:]
m = int(input())
for t in range (m):
    b = input()
    for country, city in Location.items():
        if b in city:
            print(country)
         
        
