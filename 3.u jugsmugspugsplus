num =  input()
a = num
digit = int(num)
if (digit%3==0) or (digit%5==0) or (digit%7==0) or ('3' in num) or ('5' in num) or ('7' in num):
  a =""
  if (digit%3==0) or ('3' in num):
    a ="Jugs"
  if (digit%5==0) or ('5' in num):
    a = a+"Mugs"
  if (digit%7==0) or ('7' in num) :
    a =a+"Pugs"
    
print(a)
